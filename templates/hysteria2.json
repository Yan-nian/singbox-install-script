{
  "type": "hysteria2",
  "tag": "hy2-in",
  "listen": "::",
  "listen_port": {{PORT}},
  "users": [
    {
      "password": "{{PASSWORD}}"
    }
  ],
  "tls": {
    "enabled": true,
    "server_name": "{{SERVER_NAME}}",
    "certificate_path": "{{CERT_PATH}}",
    "key_path": "{{KEY_PATH}}",
    "alpn": [
      "h3"
    ]
  },
  "masquerade": {
    "type": "proxy",
    "proxy": {
      "url": "https://{{MASQUERADE_DOMAIN}}",
      "rewrite_host": true
    }
  },
  "obfs": {
    "type": "salamander",
    "password": "{{OBFS_PASSWORD}}"
  },
  "up_mbps": {{UP_MBPS}},
  "down_mbps": {{DOWN_MBPS}},
  "ignore_client_bandwidth": false
}