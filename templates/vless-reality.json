{
  "type": "vless",
  "tag": "vless-in",
  "listen": "::",
  "listen_port": 443,
  "users": [
    {
      "uuid": "{{UUID}}",
      "flow": "xtls-rprx-vision"
    }
  ],
  "tls": {
    "enabled": true,
    "server_name": "{{SERVER_NAME}}",
    "reality": {
      "enabled": true,
      "handshake": {
        "server": "{{TARGET_SERVER}}",
        "server_port": {{TARGET_PORT}}
      },
      "private_key": "{{PRIVATE_KEY}}",
      "short_id": [
        "{{SHORT_ID}}"
      ]
    }
  },
  "transport": {
    "type": "tcp"
  }
}